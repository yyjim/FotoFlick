{
  "manifest_version": 2,
  "name": "Phlickr @Y!Hack'13",
  "version": "0.1",
  "description": "Phlickr helps to upload images you love",
  "icons": {
    "48" : "images/sample-48.png",
    "128" : "images/sample-128.png"
  },
  "permissions": [
    "tabs", "https://pic-collage.com/*", "http://pic-collage.com/*"
  ],
  "web_accessible_resources": [
    "js/*",
    "images/*"
  ],
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js" : ["js/jquery-2.0.3.min.js", "js/jquery.transit.js", "js/phlickr.js", "js/contentscript.js"]
    }
  ]
}
